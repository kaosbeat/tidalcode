        once $ ccn 1 # ccv 0 # s "midi" # midichan 3
        once $ ccn 1 # ccv 0 # s "midi" # midichan 4 -- off 0
        once $ ccn 1 # ccv 0 # s "midi" # midichan 5    
    nop5 = do
        once $ ccn 1 # ccv 0 # s "midi" # midichan 1
